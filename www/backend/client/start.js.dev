      html += '<div id="leftmenu" class="leftMenu mainArea primColor"></div>'
      $('body').html(html + '<div id="content" class="rightPart mainArea"></div>')
      requirejs(['jqxbutton'], function(mod) {
        $("#loginButton").jqxButton({ width: 40, height: 40, imgSrc: "/lib/img/login-36-white.png", imgHeight: 36, imgWidth: 36})
        $("#loginButton").css("float", 'right')
        $("#loginButton > img").css("top", '1px').css("left", '1px')
      })
/*      requirejs(['/lib/client.js'], function(mod) {
        window.client = mod.start
        window.client.init()
      })*/
    }
  }
})
